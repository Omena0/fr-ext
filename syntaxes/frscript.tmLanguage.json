{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Frscript",
  "scopeName": "source.fr",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#decorators"
    },
    {
      "include": "#directives"
    },
    {
      "include": "#c-interop"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#fstrings"
    },
    {
      "include": "#bytes"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#function-declaration"
    },
    {
      "include": "#types"
    },
    {
      "include": "#python-interop"
    },
    {
      "include": "#wasm-api"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#variable-declaration"
    },
    {
      "include": "#function-call"
    },
    {
      "include": "#struct-declaration"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#punctuation"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.test.fr",
          "begin": "^\\s*(//)(\\s*)(!)\\s*",
          "end": "$",
          "beginCaptures": {
            "1": {
              "name": "punctuation.definition.comment.fr"
            },
            "3": {
              "name": "keyword.other.test-marker.fr"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.test.fr",
              "match": "\\\\[nrt]"
            }
          ]
        },
        {
          "name": "comment.line.double-slash.fr",
          "begin": "//",
          "end": "$",
          "patterns": [
            {
              "name": "punctuation.definition.comment.fr",
              "match": "//"
            }
          ]
        }
      ]
    },
    "decorators": {
      "patterns": [
        {
          "name": "meta.decorator.fr",
          "match": "^\\s*(@)(\\w+)(?:\\(([^)]*)\\))?",
          "captures": {
            "1": {
              "name": "punctuation.definition.decorator.fr"
            },
            "2": {
              "name": "entity.name.function.decorator.fr"
            },
            "3": {
              "name": "meta.decorator.arguments.fr"
            }
          }
        }
      ]
    },
    "directives": {
      "patterns": [
        {
          "name": "meta.embedded.block.bytecode",
          "begin": "^\\s*(#bytecode)\\b",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.directive.fr"
            }
          },
          "end": "^\\s*(#bytecode\\s+end)\\b",
          "endCaptures": {
            "1": {
              "name": "keyword.control.directive.fr"
            }
          },
          "patterns": [
            {
              "include": "source.frbc"
            }
          ]
        },
        {
          "name": "keyword.control.directive.fr",
          "match": "^\\s*#(label|global|pragma)\\b"
        }
      ]
    },
    "c-interop": {
      "patterns": [
        {
          "name": "keyword.control.import.c.fr",
          "match": "^\\s*(c_import|c_link)\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.fr",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|')"
            }
          ]
        },
        {
          "name": "string.quoted.single.fr",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|')"
            }
          ]
        }
      ]
    },
    "fstrings": {
      "patterns": [
        {
          "name": "string.interpolated.fr",
          "begin": "f\"",
          "end": "(?<!\\\\)\"",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.fr"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.fr"
            }
          },
          "patterns": [
            {
              "include": "#fstring-interpolation"
            },
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|'|\\{|\\})"
            }
          ]
        },
        {
          "name": "string.interpolated.fr",
          "begin": "f'",
          "end": "(?<!\\\\)'",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.fr"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.fr"
            }
          },
          "patterns": [
            {
              "include": "#fstring-interpolation"
            },
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|'|\\{|\\})"
            }
          ]
        }
      ]
    },
    "fstring-interpolation": {
      "patterns": [
        {
          "name": "meta.embedded.expression.fr",
          "begin": "\\{",
          "end": "\\}",
          "beginCaptures": {
            "0": {
              "name": "punctuation.section.embedded.begin.fr"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.section.embedded.end.fr"
            }
          },
          "patterns": [
            {
              "include": "#fstrings"
            },
            {
              "include": "#expression"
            }
          ]
        }
      ]
    },
    "bytes": {
      "patterns": [
        {
          "name": "string.quoted.double.bytes.fr",
          "begin": "b\"",
          "end": "\"",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.fr"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.fr"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|')"
            }
          ]
        },
        {
          "name": "string.quoted.single.bytes.fr",
          "begin": "b'",
          "end": "'",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.string.begin.fr"
            }
          },
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.string.end.fr"
            }
          },
          "patterns": [
            {
              "name": "constant.character.escape.fr",
              "match": "\\\\(n|t|r|\\\\|\"|')"
            }
          ]
        }
      ]
    },
    "expression": {
      "patterns": [
        {
          "include": "#strings"
        },
        {
          "include": "#numbers"
        },
        {
          "include": "#keywords"
        },
        {
          "include": "#constants"
        },
        {
          "include": "#operators"
        },
        {
          "include": "#function-call"
        },
        {
          "name": "variable.other.fr",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.float.fr",
          "match": "\\b[0-9]+\\.[0-9]+\\b"
        },
        {
          "name": "constant.numeric.integer.fr",
          "match": "\\b[0-9]+\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.fr",
          "match": "\\b(if|elif|else|while|for|in|switch|case|default|break|continue|return|assert|try|except|raise|goto)\\b"
        },
        {
          "name": "keyword.other.fr",
          "match": "\\b(const|global)\\b"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "storage.type.fr",
          "match": "\\b(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function)\\b"
        }
      ]
    },
    "python-interop": {
      "patterns": [
        {
          "name": "keyword.other.python.fr",
          "match": "\\b(py_import|py_call|from)\\b"
        },
        {
          "name": "keyword.control.import.fr",
          "match": "\\b(as)\\b"
        }
      ]
    },
    "wasm-api": {
      "patterns": [
        {
          "comment": "DOM manipulation functions",
          "name": "support.function.dom.fr",
          "match": "\\b(dom_query|dom_query_all|dom_create|dom_get_body|dom_get_document|dom_set_text|dom_get_text|dom_set_html|dom_get_html|dom_set_attr|dom_get_attr|dom_remove_attr|dom_append|dom_prepend|dom_remove|dom_clone|dom_parent|dom_children|dom_add_class|dom_remove_class|dom_toggle_class|dom_has_class|dom_set_style|dom_get_style|dom_get_value|dom_set_value|dom_focus|dom_blur|dom_on|dom_off)\\b"
        },
        {
          "comment": "Event handling functions",
          "name": "support.function.event.fr",
          "match": "\\b(event_prevent_default|event_stop_propagation|event_target)\\b"
        },
        {
          "comment": "Timer functions",
          "name": "support.function.timer.fr",
          "match": "\\b(set_timeout|set_interval|clear_timeout|clear_interval)\\b"
        },
        {
          "comment": "Console functions",
          "name": "support.function.console.fr",
          "match": "\\b(console_log|console_error|console_warn)\\b"
        },
        {
          "comment": "Browser dialog functions",
          "name": "support.function.dialog.fr",
          "match": "\\b(alert|confirm|prompt)\\b"
        },
        {
          "comment": "Location and storage functions",
          "name": "support.function.browser.fr",
          "match": "\\b(get_location_href|set_location_href|get_local_storage|set_local_storage|remove_local_storage)\\b"
        },
        {
          "comment": "Fetch API functions",
          "name": "support.function.fetch.fr",
          "match": "\\b(fetch_text|fetch_json)\\b"
        },
        {
          "comment": "JavaScript interop functions",
          "name": "support.function.js-interop.fr",
          "match": "\\b(js_call|js_eval|js_get_global|js_set_global)\\b"
        },
        {
          "comment": "Performance functions",
          "name": "support.function.performance.fr",
          "match": "\\b(performance_now)\\b"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.language.boolean.fr",
          "match": "\\b(true|false)\\b"
        },
        {
          "name": "constant.language.null.fr",
          "match": "\\b(null|none|None)\\b"
        }
      ]
    },
    "variable-declaration": {
      "patterns": [
        {
          "match": "\\b([A-Z][a-zA-Z0-9_]*)\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*=",
          "captures": {
            "1": {
              "name": "storage.type.struct.fr"
            },
            "2": {
              "name": "variable.other.fr"
            }
          }
        }
      ]
    },
    "function-declaration": {
      "patterns": [
        {
          "match": "\\b(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\(([^)]*)\\)",
          "captures": {
            "1": {
              "name": "storage.type.function.fr"
            },
            "2": {
              "name": "entity.name.function.fr"
            },
            "3": {
              "patterns": [
                {
                  "match": "(\\*\\*)(\\s*)(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)?(\\s*)([a-zA-Z_][a-zA-Z0-9_]*)",
                  "captures": {
                    "1": {
                      "name": "keyword.operator.unpacking.kwargs.fr"
                    },
                    "3": {
                      "name": "storage.type.fr"
                    },
                    "5": {
                      "name": "variable.parameter.fr"
                    }
                  }
                },
                {
                  "match": "(\\*)(\\s*)(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)?(\\s*)([a-zA-Z_][a-zA-Z0-9_]*)",
                  "captures": {
                    "1": {
                      "name": "keyword.operator.unpacking.args.fr"
                    },
                    "3": {
                      "name": "storage.type.fr"
                    },
                    "5": {
                      "name": "variable.parameter.fr"
                    }
                  }
                },
                {
                  "match": "(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)\\s+([a-zA-Z_][a-zA-Z0-9_]*)",
                  "captures": {
                    "1": {
                      "name": "storage.type.fr"
                    },
                    "2": {
                      "name": "variable.parameter.fr"
                    }
                  }
                },
                {
                  "match": ",",
                  "name": "punctuation.separator.parameter.fr"
                },
                {
                  "match": "[a-zA-Z_][a-zA-Z0-9_]*",
                  "name": "variable.parameter.fr"
                }
              ]
            }
          }
        }
      ]
    },
    "function-parameters": {
      "patterns": [
        {
          "match": "(\\*\\*)(\\s*)(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)?(\\s*)([a-zA-Z_][a-zA-Z0-9_]*)",
          "captures": {
            "1": {
              "name": "keyword.operator.unpacking.kwargs.fr"
            },
            "3": {
              "name": "storage.type.fr"
            },
            "5": {
              "name": "variable.parameter.fr"
            }
          }
        },
        {
          "match": "(\\*)(\\s*)(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)?(\\s*)([a-zA-Z_][a-zA-Z0-9_]*)",
          "captures": {
            "1": {
              "name": "keyword.operator.unpacking.args.fr"
            },
            "3": {
              "name": "storage.type.fr"
            },
            "5": {
              "name": "variable.parameter.fr"
            }
          }
        },
        {
          "match": "\\s*(void|int|float|str|string|bool|list|dict|set|bytes|any|pyobject|pyobj|function|[A-Z][a-zA-Z0-9_]*)\\s+([a-zA-Z_][a-zA-Z0-9_]*)",
          "captures": {
            "1": {
              "name": "storage.type.fr"
            },
            "2": {
              "name": "variable.parameter.fr"
            }
          }
        },
        {
          "match": ",",
          "name": "punctuation.separator.parameter.fr"
        },
        {
          "name": "variable.parameter.fr",
          "match": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "function-call": {
      "patterns": [
        {
          "match": "\\b([A-Z][a-zA-Z0-9_]*)\\s*(?=\\()",
          "captures": {
            "1": {
              "name": "meta.embedded.line.fr"
            }
          }
        },
        {
          "name": "meta.function-call.fr",
          "begin": "\\b([a-z_][a-zA-Z0-9_]*)\\s*\\(",
          "beginCaptures": {
            "1": {
              "name": "entity.name.function.call.fr"
            }
          },
          "end": "\\)",
          "patterns": [
            {
              "include": "#expression"
            },
            {
              "include": "#strings"
            },
            {
              "include": "#fstrings"
            },
            {
              "include": "#numbers"
            },
            {
              "include": "#constants"
            }
          ]
        }
      ]
    },
    "struct-declaration": {
      "patterns": [
        {
          "name": "meta.struct.fr",
          "begin": "\\b(struct)\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\{",
          "beginCaptures": {
            "1": {
              "name": "storage.type.struct.fr"
            },
            "2": {
              "name": "entity.name.type.struct.fr"
            }
          },
          "end": "\\}",
          "patterns": [
            {
              "include": "#types"
            },
            {
              "include": "#comments"
            }
          ]
        },
        {
          "name": "storage.type.struct.fr",
          "match": "\\b(struct)\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.range.fr",
          "match": "\\.\\."
        },
        {
          "name": "keyword.operator.membership.fr",
          "match": "\\b(in|not\\s+in)\\b"
        },
        {
          "name": "keyword.operator.logical.fr",
          "match": "(&&|\\|\\||!|\\band\\b|\\bor\\b|\\bnot\\b)"
        },
        {
          "name": "keyword.operator.comparison.fr",
          "match": "(==|!=|<=|>=|<|>)"
        },
        {
          "name": "keyword.operator.assignment.fr",
          "match": "(=|\\+=|-=|\\*=|/=|%=|&=|\\|=|\\^=|<<=|>>=)"
        },
        {
          "name": "keyword.operator.arithmetic.fr",
          "match": "(\\+|-|\\*|/|%)"
        },
        {
          "name": "keyword.operator.bitwise.fr",
          "match": "(&|\\||\\^|<<|>>|~)"
        }
      ]
    },
    "punctuation": {
      "patterns": [
        {
          "name": "punctuation.separator.comma.fr",
          "match": ","
        },
        {
          "name": "punctuation.terminator.statement.fr",
          "match": ";"
        },
        {
          "name": "punctuation.accessor.fr",
          "match": "\\."
        },
        {
          "name": "punctuation.section.brackets.fr",
          "match": "[\\[\\]\\{\\}\\(\\)]"
        }
      ]
    }
  }
}
